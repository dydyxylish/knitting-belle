{
	"name": "knitting_belle",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"compilerOptions": {
		"esModuleInterop": true,
		"module": "ESNext",
		"moduleResolution": "NodeNext",
		"outDir": "dist",
		"strict": true,
		"target": "ESNext"
	},
	"engines": {
		"node": ">=20",
		"pnpm": ">=10"
	},
	"packageManager": "pnpm@10.12.1",
	"scripts": {
		"act:pr": "act pull_request -e .act/event/pr-synchronize.json > .act/log/act.pr-synchronize.log 2>&1",
		"act:pr:closed": "act pull_request -e .act/event/pr-closed.json > .act/log/act.pr-closed.log 2>&1",
		"act:push:develop": "act push -e .act/event/push-develop.json > .act/log/act.push-develop.log 2>&1",
		"ampx": "pnpm dotenv ampx",
		"build": "pnpm dotenv next build",
		"create:admin-users": "pnpm dotenv tsx amplify/script/createAdminUsers.ts",
		"dev": "rm -rf .next && pnpm dotenv next dev --turbo",
		"dotenv": "dotenvx run --convention=flow --",
		"e2e": "playwright test",
		"e2e:debug": "playwright test --debug",
		"e2e:headed": "playwright test --headed",
		"e2e:ui": "playwright test --ui",
		"lint": "next lint",
		"pm2:start": "pnpm pm2 start 'pnpm start' -n e2e-server",
		"pm2:stop": "pnpm pm2 delete e2e-server || true",
		"sandbox:create": "pnpm ampx sandbox",
		"sandbox:delete": "pnpm ampx sandbox delete",
		"sandbox:delete_secret": "pnpm ampx sandbox secret remove",
		"sandbox:seed": "pnpm ampx sandbox seed --debug",
		"sandbox:set_secret": "pnpm ampx sandbox secret set",
		"sst:deploy": "pnpm dotenv sst deploy",
		"deploy:managed-login": "cdk deploy ManagedLoginStack --require-approval never",
		"destroy:managed-login": "cdk destroy ManagedLoginStack --force",
		"diff:managed-login": "cdk diff ManagedLoginStack",
		"start": "pnpm dotenv next start",
		"test": "jest",
		"test:ci": "pnpm dotenv jest --coverage --ci --watchAll=false",
		"test:coverage": "pnpm dotenv jest --coverage",
		"test:watch": "pnpm dotenv jest --watch",
		"wait:e2e": "pnpm wait-on http://localhost:3000 -t 30000"
	},
	"dependencies": {
		"@aws-amplify/adapter-nextjs": "^1.6.4",
		"@aws-sdk/client-cognito-identity-provider": "^3.839.0",
		"@conform-to/react": "^1.7.2",
		"@conform-to/zod": "^1.7.2",
		"@lottiefiles/react-lottie-player": "^3.6.0",
		"@radix-ui/react-accordion": "^1.2.11",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-scroll-area": "^1.2.9",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@tailwindcss/postcss": "^4.1.10",
		"aws-amplify": "^6.15.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dataloader": "^2.2.3",
		"dayjs": "^1.11.13",
		"embla-carousel-autoplay": "^8.6.0",
		"embla-carousel-fade": "^8.6.0",
		"embla-carousel-react": "^8.6.0",
		"es-toolkit": "^1.39.4",
		"lottie-react": "^2.4.1",
		"lucide-react": "^0.515.0",
		"next": "15.3.4",
		"next-themes": "^0.4.6",
		"p-retry": "^6.2.1",
		"pino": "^9.7.0",
		"postcss": "^8.5.6",
		"react": "^19.1.0",
		"react-cookie": "^8.0.1",
		"react-dom": "^19.1.0",
		"react-resizable-panels": "^3.0.3",
		"server-only": "^0.0.1",
		"sonner": "^2.0.6",
		"sst": "3.17.13",
		"stripe": "^18.2.1",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^3.4.17",
		"uuid": "^11.1.0",
		"zod": "3.25.17"
	},
	"devDependencies": {
		"@aws-amplify/backend": "^1.16.1",
		"@aws-amplify/backend-cli": "^1.8.0",
		"@aws-amplify/seed": "^1.0.1",
		"@aws-sdk/client-dynamodb": "^3.868.0",
		"@aws-sdk/lib-dynamodb": "^3.868.0",
		"@biomejs/biome": "2.0.6",
		"@dotenvx/dotenvx": "^1.45.1",
		"@faker-js/faker": "^10.0.0",
		"@jest/globals": "^30.0.5",
		"@opennextjs/aws": "^3.7.7",
		"@playwright/test": "^1.55.0",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/react": "^16.3.0",
		"@types/aws-lambda": "^8.10.150",
		"@types/jest": "^30.0.0",
		"@types/node": "^22.15.32",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"autoprefixer": "^10.4.21",
		"aws-cdk": "^2.1019.1",
		"aws-cdk-lib": "^2.202.0",
		"constructs": "^10.4.2",
		"dynamodb-local": "^0.0.35",
		"esbuild": "^0.25.5",
		"fast-glob": "^3.3.3",
		"jest": "^30.0.5",
		"jest-environment-jsdom": "^29.7.0",
		"jest-junit": "^16.0.0",
		"lefthook": "^1.11.14",
		"pino-pretty": "^13.0.0",
		"pm2": "^6.0.8",
		"ts-jest": "^29.4.1",
		"ts-node": "^10.9.2",
		"tsx": "^4.20.3",
		"tw-animate-css": "^1.3.4",
		"typescript": "^5.8.3",
		"wait-on": "^8.0.4",
		"yaml": "^2.8.0"
	}
}
