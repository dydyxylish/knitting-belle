{
	"app": "pnpm tsx amplify/config/managedLoginApp.ts",
	"watch": {
		"include": ["amplify/config/**/*.ts", "amplify/auth/**/*.json"],
		"exclude": [
			"README.md",
			"cdk*.json",
			"**/*.d.ts",
			"**/*.js.map",
			"node_modules/**",
			".git/**"
		]
	},
	"context": {
		"@aws-cdk/aws-lambda:recognizeLayerVersion": true,
		"@aws-cdk/core:checkSecretUsage": true,
		"@aws-cdk/core:target": "aws-cdk-lib",
		"@aws-cdk-lib/aws-s3:createDefaultLoggingPolicy": true,
		"@aws-cdk/aws-route53-pathalias:enabled": true,
		"aws-cdk:enableDiffNoFail": true,
		"@aws-cdk/core:stackRelativeExports": true,
		"@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
		"@aws-cdk/aws-lambda:recognizeVersionProps": true,
		"@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
		"@aws-cdk-lib/aws-rds:databaseProxyUniqueResourceName": true
	},
	"toolkitStackName": "CDKToolkit-ManagedLogin",
	"profile": "default"
}
